# Example secret configuration for Ultralytics
# Copy this file to secret.yaml and fill in your values
# Apply with: kubectl apply -f secret.yaml
#
# Note: In production, consider using external secret management
# such as HashiCorp Vault, AWS Secrets Manager, or Kubernetes External Secrets

apiVersion: v1
kind: Secret
metadata:
  name: ultralytics-secrets
type: Opaque
stringData:
  # Database connection string
  DATABASE_URL: "postgresql://user:password@postgres:5432/ultralytics"
  
  # Master API key for administrative operations
  MASTER_API_KEY: "your-master-api-key-here"
