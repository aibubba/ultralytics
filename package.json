{
  "name": "ultralytics",
  "version": "1.1.0",
  "description": "Self-hosted analytics for web applications",
  "main": "dist/server.js",
  "module": "dist/ultralytics.esm.js",
  "browser": "dist/ultralytics.min.js",
  "types": "dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ultralytics.esm.js",
      "require": "./dist/ultralytics.js",
      "browser": "./dist/ultralytics.min.js",
      "types": "./dist/types.d.ts"
    },
    "./server": {
      "require": "./dist/server.js",
      "types": "./dist/server.d.ts"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "build": "tsc && rollup -c",
    "build:server": "tsc",
    "build:client": "rollup -c",
    "cleanup": "node scripts/cleanup.js",
    "cleanup:cron": "node scripts/cleanup.js --cron",
    "migrate": "node-pg-migrate",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "benchmark": "ts-node benchmarks/ingestion.bench.ts && ts-node benchmarks/queries.bench.ts && ts-node benchmarks/client.bench.ts",
    "benchmark:ingestion": "ts-node benchmarks/ingestion.bench.ts",
    "benchmark:queries": "ts-node benchmarks/queries.bench.ts",
    "benchmark:client": "ts-node benchmarks/client.bench.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aibubba/ultralytics.git"
  },
  "keywords": [
    "analytics",
    "self-hosted",
    "privacy"
  ],
  "author": "bubba",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aibubba/ultralytics/issues"
  },
  "homepage": "https://github.com/aibubba/ultralytics#readme",
  "dependencies": {
    "ajv": "8.12.0",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-rate-limit": "7.1.4",
    "helmet": "7.1.0",
    "morgan": "1.10.0",
    "node-cron": "3.0.3",
    "node-pg-migrate": "6.2.2",
    "pg": "8.11.3",
    "prom-client": "15.0.0",
    "sanitize-html": "2.11.0",
    "swagger-ui-express": "5.0.0",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "11.1.5",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@types/node": "20.9.0",
    "@types/pg": "8.10.9",
    "@types/react": "18.2.37",
    "@types/sanitize-html": "2.9.5",
    "@types/swagger-ui-express": "4.1.6",
    "@types/yamljs": "0.2.34",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "@playwright/test": "1.40.1",
    "rollup": "4.3.0",
    "rollup-plugin-terser": "7.0.2",
    "supertest": "6.3.3",
    "ts-node": "10.9.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "svelte": ">=3.0.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  }
}
