{
  "name": "ultralytics",
  "version": "0.4.0",
  "description": "Self-hosted analytics for web applications",
  "main": "dist/server.js",
  "module": "dist/ultralytics.esm.js",
  "browser": "dist/ultralytics.min.js",
  "types": "dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ultralytics.esm.js",
      "require": "./dist/ultralytics.js",
      "browser": "./dist/ultralytics.min.js",
      "types": "./dist/types.d.ts"
    },
    "./server": {
      "require": "./dist/server.js",
      "types": "./dist/server.d.ts"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "build": "tsc && rollup -c",
    "build:server": "tsc",
    "build:client": "rollup -c",
    "cleanup": "node scripts/cleanup.js",
    "cleanup:cron": "node scripts/cleanup.js --cron",
    "migrate": "node-pg-migrate",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aibubba/ultralytics.git"
  },
  "keywords": [
    "analytics",
    "self-hosted",
    "privacy"
  ],
  "author": "bubba",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aibubba/ultralytics/issues"
  },
  "homepage": "https://github.com/aibubba/ultralytics#readme",
  "dependencies": {
    "ajv": "6.12.6",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-rate-limit": "5.2.3",
    "morgan": "1.10.0",
    "node-cron": "2.0.3",
    "node-pg-migrate": "5.9.0",
    "pg": "8.5.1",
    "prom-client": "14.0.1",
    "sanitize-html": "2.7.0",
    "swagger-ui-express": "4.1.6",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "8.2.0",
    "@types/express": "4.17.11",
    "@types/express-rate-limit": "5.1.1",
    "@types/morgan": "1.9.2",
    "@types/node": "14.14.22",
    "@types/pg": "7.14.9",
    "@types/react": "17.0.3",
    "@types/sanitize-html": "2.6.2",
    "@types/swagger-ui-express": "4.1.2",
    "@types/yamljs": "0.2.31",
    "jest": "26.6.3",
    "jest-environment-jsdom": "26.6.2",
    "rollup": "2.33.3",
    "rollup-plugin-terser": "7.0.2",
    "supertest": "6.0.1",
    "ts-node": "9.1.1",
    "tslib": "2.1.0",
    "typescript": "4.1.5"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
