{
  "name": "ultralytics",
  "version": "0.5.0",
  "description": "Self-hosted analytics for web applications",
  "main": "dist/server.js",
  "module": "dist/ultralytics.esm.js",
  "browser": "dist/ultralytics.min.js",
  "types": "dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ultralytics.esm.js",
      "require": "./dist/ultralytics.js",
      "browser": "./dist/ultralytics.min.js",
      "types": "./dist/types.d.ts"
    },
    "./server": {
      "require": "./dist/server.js",
      "types": "./dist/server.d.ts"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "build": "tsc && rollup -c",
    "build:server": "tsc",
    "build:client": "rollup -c",
    "cleanup": "node scripts/cleanup.js",
    "cleanup:cron": "node scripts/cleanup.js --cron",
    "migrate": "node-pg-migrate",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aibubba/ultralytics.git"
  },
  "keywords": [
    "analytics",
    "self-hosted",
    "privacy"
  ],
  "author": "bubba",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aibubba/ultralytics/issues"
  },
  "homepage": "https://github.com/aibubba/ultralytics#readme",
  "dependencies": {
    "ajv": "8.12.0",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-rate-limit": "6.7.0",
    "helmet": "6.0.1",
    "morgan": "1.10.0",
    "node-cron": "3.0.2",
    "node-pg-migrate": "6.2.2",
    "pg": "8.10.0",
    "prom-client": "14.2.0",
    "sanitize-html": "2.10.0",
    "swagger-ui-express": "4.6.2",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "11.0.0",
    "@types/express": "4.17.17",
    "@types/morgan": "1.9.4",
    "@types/node": "18.15.0",
    "@types/pg": "8.6.6",
    "@types/react": "18.0.28",
    "@types/sanitize-html": "2.9.0",
    "@types/swagger-ui-express": "4.1.3",
    "@types/yamljs": "0.2.31",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "rollup": "3.20.0",
    "rollup-plugin-terser": "7.0.2",
    "supertest": "6.3.3",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "5.0.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
